{
  "name": "j0ntsang.github.io",
  "version": "0.0.1",
  "homepage": "https://j0ntsang.github.io/j0ntsang.github.io",
  "private": true,
  "scripts": {
    "build:terminal": "vite build",
    "build": "npm run build:terminal && react-scripts build",
    "start": "concurrently \"npm run dev:terminal\" \"react-scripts start\"",
    "dev:terminal": "vite build --watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^22.15.29",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "autoprefixer": "^10.4.7",
    "concurrently": "^9.1.2",
    "gh-pages": "^3.2.3",
    "nanoid": "^4.0.0",
    "postcss": "^8.4.14",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.5.4",
    "vite": "^6.3.5"
  }
}
